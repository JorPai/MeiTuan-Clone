.active {
  background-color: #fbae22 !important;
}
.searchBox {
  margin: 0 15rpx;
}
.section-nav {
  display: flex;
  align-items: center;
  gap: 15rpx;
  margin-top: 15rpx;
}
.section-nav image {
  width: 35rpx;
  height: 35rpx;
  background-color: yellow;
}
.section-nav .nav-text {
  font-weight: bold;
  font-size: 32rpx;
}
.section-nav ._span {
  font-size: 24rpx;
  color: #cec1c1;
}
.section-search {
  position: relative;
  margin: 20rpx 0;
}
.section-search image {
  width: 35rpx;
  height: 35rpx;
  position: absolute;
  top: 30%;
  left: 2%;
}
.section-search input {
  background-color: antiquewhite;
  border-radius: 8rpx;
  padding: 12rpx 60rpx;
  font-size: 32rpx;
}
.section-swiper swiper {
  height: 320rpx !important;
}
.section-swiper swiper .swiper-item {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-between;
}
.section-swiper swiper .swiper-item .conteng-img {
  width: calc((100% / 5) - 12px) !important;
  margin: 12rpx;
  text-align: center;
}
.section-swiper swiper .swiper-item .conteng-img .uploadimg {
  width: 70rpx;
  height: 70rpx;
  margin: 0 10rpx;
}
.section-swiper swiper .swiper-item .conteng-img text {
  display: block;
  padding-top: 10rpx;
  font-size: 28rpx;
}
.section-swiper swiper .swiper-item .conteng-img:nth-child(n+6) {
  padding-top: 16rpx;
}
.section-swiper .list-doit {
  display: flex;
  justify-content: center;
  gap: 20rpx;
  padding-top: 16rpx;
}
.section-swiper .list-doit .doit {
  width: 60rpx;
  height: 12rpx;
  background-color: #e6e6e6;
  border-radius: 8rpx;
}

