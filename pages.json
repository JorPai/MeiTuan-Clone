{
	"easycom": {
	  "autoscan": true,
	  "custom": {
	    "^uni-(.*)": "@/App.vue/uni-$1.vue", // 匹配components目录内的vue文件
	    "^vue-file-(.*)": "packageName/path/to/vue-file-$1.vue" // 匹配node_modules内的vue文件
	  }
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "美团外卖"
			}
		},
		{
			"path": "pages/order/order",
			"style": {
				"navigationBarTitleText": "订单"
			}
		},
		{
			"path": "pages/myself/myself",
			"style": {
				"navigationBarTitleText": "我的"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "美团外卖",
		"navigationBarBackgroundColor": "#ffff00",
		"backgroundColor": "#ffff00"
	},
	"tabBar": {
		"color": "#999794",
		"selectedColor": "#33312e",
		"borderStyle":"white",
		"backgroundColor": "#fafafa",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/tab/shouyea.png",
			"selectedIconPath": "static/tab/shouyeb.png",
			"text": "首页"
		}, {
			"pagePath": "pages/order/order",
			"iconPath": "static/tab/dingdana.png",
			"selectedIconPath": "static/tab/dingdanb.png",
			"text": "订单"
		},{
			"pagePath": "pages/myself/myself",
			"iconPath": "static/tab/wodea.png",
			"selectedIconPath": "static/tab/wodeb.png",
			"text": "订单"
		}
		]
	},
	"uniIdRouter": {}
}
